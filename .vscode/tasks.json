{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "run",
			"env": {
				"RUST_BACKTRACE": "1"
			},
			"problemMatcher": [
				"$rustc"
			],
			"label": "rust: cargo run debug"
		},
		{
			"type": "cargo",
			"command": "run",
			"args" : ["--release"],
			"problemMatcher": [
				"$rustc"
			],
			"label": "rust: cargo run release"
		}
	]
}